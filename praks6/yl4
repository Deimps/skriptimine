#!/bin/bash
# Ãœl4 v2: prindi 1..20 iga arvu kohta, kas on algarv.

is_prime() {
  local n="$1"
  (( n >= 2 )) || return 1
  local j=2
  while (( j * j <= n )); do
    (( n % j == 0 )) && return 1
    (( j++ ))
  done
  return 0
}

for x in {1..20}; do
  if is_prime "$x"; then
    echo "$x - on algarv"
  else
    echo "$x - ei ole algarv"
  fi
done
